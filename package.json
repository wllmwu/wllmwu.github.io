{
  "name": "william-wu-site",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "imports": {
    "#components": "./components"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives",
    "export": "next export",
    "deploy": "next build && next export && cp CNAME out/ && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.2",
    "@next/mdx": "^12.3.1",
    "next": "^12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-frontmatter": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-next": "^13.0.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "typescript": "^4.9.3"
  }
}
