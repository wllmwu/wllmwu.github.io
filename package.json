{
  "name": "william-wu-site",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "imports": {
    "#components": "./components"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --max-warnings 0 --report-unused-disable-directives",
    "export": "next export",
    "deploy": "next build && next export && cp CNAME out/ && touch out/.nojekyll && git add out/ && git commit -m \"Deploy Next.js to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.2",
    "@next/mdx": "^12.1.6",
    "next": "^12.1.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "remark-frontmatter": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.26",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.0",
    "typescript": "^4.6.3"
  }
}
